{
  "name": "famile-brev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node build/backend/server.js",
    "start:dev": "yarn dev:server",
    "test": "jest --watchAll --no-cache",
    "eject": "react-scripts eject",
    "dev:server": "concurrently -p name -c \"yellow,magenta\" -n \"webpack-server,nodemon-server\" \"yarn dev:server:webpack\" \"yarn dev:server:nodemon\"",
    "dev:server:webpack": "webpack --config webpack.config.server.js --watch",
    "dev:server:nodemon": "rm -f build/server.js && wait-on -l build/backend/server.js && nodemon build/backend/server.js --delay 500ms",
    "build": "yarn build:server",
    "build:server": "webpack --config webpack.config.server.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "eslint --fix src/"
    ]
  },
  "dependencies": {
    "@navikt/familie-backend": "^7.0.4",
    "@navikt/familie-logging": "^1.0.2",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^3.1.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.20",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@typescript-eslint/typescript-estree": "^5.30.5",
    "axios": "^0.26.0",
    "babel-jest": "^27.0.6",
    "concurrently": "^7.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jest": "^27.4.3",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "wait-on": "^5.3.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.9.52",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.0"
  }
}
