{
  "name": "famile-brev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node build/backend/server.js",
    "start:dev": "yarn dev:server",
    "test": "jest --watchAll --no-cache",
    "eject": "react-scripts eject",
    "dev:server": "concurrently -p name -c \"yellow,magenta\" -n \"webpack-server,nodemon-server\" \"yarn dev:server:webpack\" \"yarn dev:server:nodemon\"",
    "dev:server:webpack": "webpack --config webpack.config.server.js --watch",
    "dev:server:nodemon": "rm -f build/server.js && wait-on -l build/backend/server.js && nodemon build/backend/server.js --delay 500ms",
    "build": "yarn build:server",
    "build:server": "webpack --config webpack.config.server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "eslint --fix src/"
    ]
  },
  "dependencies": {
    "@navikt/familie-backend": "^5.0.24",
    "@navikt/familie-logging": "^1.0.2",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^2.0.1",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^24.0.0",
    "@types/node": "^17.0.15",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.1.4",
    "axios": "^0.24.0",
    "babel-jest": "^27.0.6",
    "concurrently": "^6.5.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react-app": "^6.2.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "husky": "^4.2.5",
    "jest": "^27.4.3",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.3.0",
    "typescript": "~3.7.2",
    "wait-on": "^5.3.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.9.52",
    "gh-pages": "^3.2.3"
  }
}
